      "source": [
        "def sumofInt(arr):\n",
        "  ans = 0 # for storing the addtion of the element which is multiples by 3\n",
        "\n",
        "  for i in arr:\n",
        "    if i % 3 == 0:\n",
        "      ans += i\n",
        "  return ans\n",
        "\n",
        "# driver code:\n",
        "n = int(input())\n",
        "arr = list(map(int,input().strip().split()))[:n]\n",
        "\n",
        "# call the function:\n",
        "sumofInt(arr)"
      ]
