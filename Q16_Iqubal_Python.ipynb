        "def split_join(string):\n",
        "  string = string.split(\" \") # split the string with the white space.\n",
        "  string = \"-\".join(string)\n",
        "  return string\n",
        "\n",
        "string = input(\"Enter the string for perform operation: \")\n",
        "result = split_join(string)\n",
        "print(result)\n"
